		}
}
 float calculate4 (int EFSCl, int EFCCl, int EFECl, int EFMCl, int EFCTCl, int EFICl, float CMSCl, float CMCCl, float CMECl, float CMMCl, float CMCTCl, float CMICl)
{
	     int CECl;
	     float CMUCl, PMCl;
   	   //Calcular el total de estudiantes en la Universidad
   	     CECl=EFSCl+EFCCl+EFECl+EFMCl+EFCTCl+EFICl;
   	     
   	     //Calcular el total de costo de matrícula de la Universidad
   	     CMUCl=CMSCl+CMCCl+CMECl+CMMCl+CMCTCl+CMICl;
   	     
   	     //Calcular el promedio de matricula
   	     PMCl=CMUCl/CECl;
   	     
   	     return CMUCl;
   	}
   	
   	
	  
