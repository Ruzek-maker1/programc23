   int matricula_8 (int EFSB, int EFCB, int EFEB, int EFMB, int EFCTB, int EFIB)
   {
   float CMSB, CMCB, CMEB, CMMB, CMCTB, CMIB;
   	//Calcular el total de costo de matrícula de cada Facultad
   	if (FAC=='a')
   	{
	   EFSB=EFSB+1;
   	   CMSB=EFSB*CDMF;
   	   return CMSB;
   	 }  
   	   else if (FAC=='b')
   	   {
		EFCB=EFCB+1;  
   	   CMCB=EFCB*CDMF;
   	   return CMCB;
   	   }
   	   else if (FAC=='c')
   	   {
		EFEB=EFEB+1;  
   	   CMEB=EFEB*CDMF;
   	   return CMEB;
   	   }
   	   else if (FAC=='d')
   	   {
		EFMB=EFMB+1;  
   	   CMMB=EFMB*CDMF;
   	   return CMMB;
   	   }
   	   else if (FAC=='e')
   	   {
		EFCTB=EFCTB+1;  
   	   CMCTB=EFCTB*CDMF;
   	   return CMCTB;
   	   }
   	   else if (FAC=='f')
   	   {
		EFIB=EFIB+1;  
   	   CMIB=EFIB*CDMF;
   	   return CMIB;
		}
   }
   float calculate8 (int EFSB, int EFCB, int EFEB, int EFMB, int EFCTB, int EFIB, float CMSB, float CMCB, float CMEB, float CMMB, float CMCTB, float CMIB)
   {
   	 int CEB;
   	 float CMUB, PMB;
   	   //Calcular el total de estudiantes en la Universidad
   	     CEB=EFSB+EFCB+EFEB+EFMB+EFCTB+EFIB;
   	     
   	     //Calcular el total de costo de matrícula de la Universidad
   	     CMUB=CMSB+CMCB+CMEB+CMMB+CMCTB+CMIB;
   	     
   	     //Calcular el promedio de matricula
   	     PMB=CMUB/CEB;	
   	     
   	     return CMUB;
	} 		 
    
    float calculatet (float CMUS, float CMUO,  float CMUC, float CMUCl, float CMUA, float CMUV, float CMUCh, float CMUB)
    {
     float TCMTU;
    //Calcular el total de matricula en la universidad
    TCMTU=CMUS+CMUO+CMUC+CMUCl+CMUA+CMUV+CMUCh+CMUB;
}

void monto(float CM, float CMUS, float CMUO, float CMUC, float CMUCl, float CMUA, float CMUV, float CMUCh, float CMUB)
{
    //Cual sede/centro regional tiene el mayor monto de matricula
    //Calcular que regional/sede tiene el mayor monto de matricula
    if (CM>CMUO && CM>CMUC && CM>CMUCl && CM>CMUA && CM>CMUV && CM>CMUCh && CM>CMUB)
    {
    	CRMM="sede de Panama";
    	TMMCR=CMUS;
	}
    else if (CM>CMUS && CM>CMUC && CM>CMUCl && CM>CMUA && CM>CMUV && CM>CMUCh && CM>CMUB)
    {
    	CRMM="Centro Regional de Panama Oeste";
    	TMMCR=CMUO;
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUCl && CM>CMUA && CM>CMUV && CM>CMUCh && CM>CMUB)
    {
    	CRMM="Centro Regional de Colon";
    	 TMMCR=CMUC;
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUC && CM>CMUA && CM>CMUV && CM>CMUCh && CM>CMUB)
    {
    	CRMM="Centro Regional de Cocle";
    	TMMCR=CMUCl;
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUC && CM>CMUCl && CM>CMUV && CM>CMUCh && CM>CMUB)
    {
    	CRMM="Centro Regional de Azuero";
    	TMMCR=CMUA;
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUC && CM>CMUCl && CM>CMUA && CM>CMUCh && CM>CMUB)
    {
    	CRMM="Centro Regional de Veraguas";
    	 TMMCR=CMUV;
    	 
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUC && CM>CMUCl && CM>CMUA && CM>CMUV && CM>CMUB)
    {	
    	CRMM="Centro Regional de Chiriqui";
    	TMMCR=CMUCh;
	}
    else if (CM>CMUS && CM>CMUO && CM>CMUC && CM>CMUCl && CM>CMUA && CM>CMUV && CM>CMUCh)
    {
    	CRMM="Centro Regional de Bocas del Toro";
    	TMMCR=CMUB;
	}
    	
      //Calcualar que regional/sede tiene el menor monto de matricula
     //Cual centro sede/centro regional tiene el menor monto de matricula
     if (CM<CMUO && CM<CMUC && CM<CMUCl && CM<CMUA && CM<CMUV && CM<CMUCh && CM<CMUB)
     {
        CRMeM="Sede de panama";
		TMMeCR=CMUS;
    }
    else if (CM<CMUS && CM<CMUC && CM<CMUCl && CM<CMUA && CM<CMUV && CM<CMUCh && CM<CMUB)
    {
    	CRMeM="Centro Regional de Panama Oeste";
		TMMeCR=CMUO;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUCl && CM<CMUA && CM<CMUV && CM<CMUCh && CM<CMUB)
    {
    	CRMeM="Centro Regional de Colon";
    	TMMeCR=CMUC;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUC && CM<CMUA && CM<CMUV && CM<CMUCh && CM<CMUB)
    {
    	CRMeM="Centro Regional de Cocle";
    	TMMeCR=CMUCl;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUC && CM<CMUCl && CM<CMUV && CM<CMUCh && CM<CMUB)
    {
    	CRMeM="Centro Regional de Azuero";
    	TMMeCR=CMUA;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUC && CM<CMUCl && CM<CMUA && CM<CMUCh && CM<CMUB)
    {
    	CRMeM="Centro Regional de Veraguas";
    	TMMeCR=CMUV;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUC && CM<CMUCl && CM<CMUA && CM<CMUV && CM<CMUB)
    {
    	CRMeM="Centro Regional de Chiriqui";
    	TMMeCR=CMUCh;
	}
    else if (CM<CMUS && CM<CMUO && CM<CMUC && CM<CMUCl && CM<CMUA && CM<CMUV && CM<CMUCh)
    {
    	CRMeM="Centro Regional de Bocas del Toro";
    	TMMeCR=CMUB;
	}
    
}
 	 
