#include <iostream>

using namespace std;

	//Ingrese las variables globales
   string CDE, NE, CRMM, CRMeM;
   char RESE, FAC;
   int EFSS, EFCS, EFES, EFMS, EFCTS, EFIS, EFSO, EFCO, EFEO, EFMO, EFCTO, EFIO, EFSC, EFCC, EFEC, EFMC, EFCTC, EFIC, EFSCl, EFCCl, EFECl, EFMCl, EFCTCl, EFICl, EFSA, EFCA, EFEA, EFMA, EFCTA, EFIA, EFSV, EFCV, EFEV, EFMV, EFCTV, EFIV, EFSCh, EFCCh, EFECh, EFMCh, EFCTCh, EFICh, EFSB, EFCB, EFEB, EFMB, EFCTB, EFIB, Num, Num2;
   float CDMF, TMMCR, TMMeCR, TCMTU;
 
   	
   int matricula_1  (int EFSS, int EFCS, int EFES, int EFMS, int EFCTS, int EFIS)
   { 
   		float CMSS, CMCS, CMES, CMMS, CMCTS, CMIS;
   	//Calcular el total de costo de matrícula de cada Facultad
   	    if (FAC =='a')
   	    {
   	    	EFSS=EFSS+1;
   	    	CMSS=EFSS*CDMF;
   	   		return CMSS;
		}
   	    else if (FAC =='b')
   	    {
		   	EFCS=EFCS+1;
   	    	CMCS=EFCS*CDMF;
   	    	return CMCS;
		}
   	    else if (FAC =='c')
   	    {
   	    	EFES=EFES+1;
			CMES=EFES*CDMF;
   	    	return CMES;
		}   
   	    else if (FAC == 'd')
   	    {
   	    	EFMS=EFMS+1;
			CMMS=EFMS*CDMF;
   	    	return CMMS;
		}
   	    else if (FAC == 'e')
   	    {
		    EFCTS=EFCTS+1;   
   	        CMCTS=EFCTS*CDMF;
   	    return CMCTS;
   	   }
   	    else if (FAC =='e')
   	    {
		EFIS=EFIS+1;   
   	    CMIS=EFIS*CDMF;
        return CMIS;
		}
}
    float calculate1 (int EFSS, int EFCS, int EFES, int EFMS, int EFCTS, int EFIS, float CMSS, float CMCS, float CMES, float CMMS, float CMCTS, float CMIS)
     {
     	int CES;
		float CMUS, PMS;
   	   //Calcular el total de estudiantes en la Universidad
   	     CES=EFSS+EFCS+EFES+EFMS+EFCTS+EFIS;
   	     
   	     //Calcular el total de costo de matrícula de la Universidad
   	     CMUS=CMSS+CMCS+CMES+CMMS+CMCTS+CMIS;
   	     
   	     //Calcular el promedio de matricula
   	     PMS=CMUS/CES;
   	     
   	     return CMUS;
   	}   
   
   
	
   int matricula_2 (int EFSO, int EFCO, int EFEO, int EFMO, int EFCTO, int EFIO)
	 {
	 	float CMSO, CMCO, CMEO, CMMO, CMCTO, CMIO;
   	//Calcular el total de costo de matrícula de cada Facultad
   	    if (FAC=='a')
   	    {
		EFSO=EFSO+1;  
   	    CMSO=EFSO*CDMF;
   	    return CMSO;
   	   } 
   	    else if (FAC=='b')
   	   { 
   	    EFCO=EFCO+1;
   	    CMCO=EFCO*CDMF;
   	    return CMSO;
   	   }
   	    else if (FAC=='c')
   	    {
		EFEO=EFEO+1;   
   	    CMEO=EFEO*CDMF;
   	    return CMEO;
   	   }
   	    else if (FAC=='d')
   	    {
		EFMO=EFMO+1;  
   	    CMMO=EFMO*CDMF;
   	    return CMMO;
   	   }
   	    else if (FAC=='e')
   	    {
		EFCTO=EFCTO+1;   
   	    CMCTO=EFCTO*CDMF;
   	    return CMCTO;
   	   }
   	    else if (FAC=='f')
   	    {
		EFIO=EFIO+1;   
   	    CMIO=EFIO*CDMF;
   	    return CMIO;
		   }
}

float calculate2 (int EFSO, int EFCO, int EFEO, int EFMO, int EFCTO, int EFIO, float CMSO, float CMCO , float CMEO, float CMMO, float CMCTO, float CMIO)
{
		//declarar variables
		int CEO;
		float CMUO, PMO;
		
   	   //Calcular el total de estudiantes en la Universidad
   	     CEO=EFSO+EFCO+EFEO+EFMO+EFCTO+EFIO;
   	     
   	     //Calcular el total de costo de matrícula de la Universidad
   	     CMUO=CMSO+CMCO+CMEO+CMMO+CMCTO+CMIO;
   	     
   	     //Calcular el promedio de matricula
   	     PMO=CMUO/CEO;
   	     
   	     return CMUO;
   }	     
   
	   
	   
   int matricula_3 (int EFSC, int EFCC, int EFEC, int EFMC, int EFCTC, int EFIC)
   {
      float CMSC, CMCC, CMEC, CMMC, CMCTC, CMIC;
   	//Calcular el total de costo de matrícula de cada Facultad
   	if (FAC=='a')
   	{
	    EFSC=EFSC+1;
   	    CMSC=EFSC*CDMF;
   	    return CMSC;
   	 }   
   	    else if (FAC=='b')
   	    {
		 EFCC=EFCC+1;  
   	    CMCC=EFCC*CDMF;
   	    return CMCC;
   	}   
   	    else if (FAC=='c')
   	    {
		EFEC=EFEC+1;   
   	    CMEC=EFEC*CDMF;
   	    return CMEC;
    }
	
   	    else if (FAC=='d')
   	    {
		EFMC=EFMC+1;   
   	    CMMC=EFMC*CDMF;
   	    return CMMC;
   	   }
   	    else if (FAC=='e')
   	    {
		EFCTC=EFCTC+1;   
   	    CMCTC=EFCTC*CDMF;
   	    return CMCTC;
   	   }
   	    else if (FAC=='f')
   	    {
		EFIC=EFIC+1;   
   	    CMIC=EFIC*CDMF;
   	    return CMIC;
		   }
}
    float calculate3 (int EFSC, int EFCC, int EFEC, int EFMC, int EFCTC, int EFIC, float CMSC, float CMCC, float CMEC, float CMMC, float CMCTC, float CMIC)
{
	    int CEC;
	    float CMUC, PMC;
   	   //Calcular el total de estudiantes en la Universidad
   	     CEC=EFSC+EFCC+EFEC+EFMC+EFCTC+EFIC;
   	     
   	     //Calcular el total de costo de matrícula de la Universidad
   	     CMUC=CMSC+CMCC+CMEC+CMMC+CMCTC+CMIC;
   	     
   	     //Calcular el promedio de matricula
   	     PMC=CMUC/CEC;
   	     
   	     return CMUC;
   	}    

	 
   int matricula_4 (int EFSCl, int EFCCl, int EFECl, int EFACl, int EFCTCl, int EFICl)
