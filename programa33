   void students2 (int EFSO, int EFCO, int EFEO, int EFMO, int EFCTO, int EFIO)
        {
        cout<<"\n Centro Regional de Panama Oeste"<<endl;
        //Mostrar el total de estudiantes en cada Facultad
		cout<<"\n El Total de estudiantes en cada Facultad"<<endl;
