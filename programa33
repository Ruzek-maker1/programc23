     }

  
  

  int main () {
  	
  	float CM, PMS, PMO, PMC, PMCl, PMA, PMV, PMCh, PMB, CMUS, CMUO, CMUC, CMUCl, CMUA, CMUV, CMUCh, CMUB, CMSS, CMCS, CMES, CMMS, CMCTS, CMIS, CMSO, CMCO, CMEO, CMMO, CMCTO, CMIO, CMSC, CMCC, CMEC, CMMC, CMCTC, CMIC, CMSCl, CMCCl, CMECl, CMMCl, CMCTCl, CMICl, CMSA, CMCA, CMEA, CMMA, CMCTA, CMIA, CMSV, CMCV, CMEV, CMMV, CMCTV, CMIV, CMSCh, CMCCh, CMECh, CMMCh, CMCTCh, CMICh, CMSB, CMCB, CMEB, CMMB, CMCTB, CMIB;
  	int CES, CEO, CEC, CECl, CEA, CEV, CECh, CEB;
  	
  	do {
  	cout<<"\n Universidad Tecnologica de Panama"<<endl;
 	
     cout<<"Ingrese la letra del Centor Regional/Sede"<<endl;
     cout<<"a) Sede de Panama"<<endl;
     cout<<"b) Centro Regional de Panama Oeste"<<endl;
     cout<<"c) Centro Regional de Colon"<<endl;
     cout<<"d) Centro Regional de Cocle"<<endl;
     cout<<"e) Centro Regional de Azuero"<<endl;
     cout<<"f) Centro Regional de Veraguas"<<endl;
     cout<<"g) Centro Regional de Chiriqui"<<endl;
     cout<<"h) Centro Regional de Bocas del Toro"<<endl;
     cout<<"Centro Regional o Sede: "; cin>>RESE;
  
     	do{
     		
       	    cout<<"\n Ingrese la letra de la Facultad"<<endl; 
     	    cout<<"a) Facultad de Ingenieria de Sistemas y computacion"<<endl;
     	    cout<<"b) Facultad de Ingenieria Civil"<<endl;
          	cout<<"c) Facultad de Ingenieria Electrica"<<endl;
     	    cout<<"d) Facultad de Ingenieria Mecanica"<<endl;
            cout<<"e) Facultad de Ciencia y Tecnologia"<<endl;
            cout<<"f) Facultad de Ingenieria Industrial"<<endl;
     	    cout<<"Facultad de: "; cin>>FAC;
         	cout<<"Ingrese la cedula del estudiante: "; cin>>CDE;
         	cout<<"Ingrese el nombre del estudiante: "; cin>>NE;
         	cout<<"Ingrese el costo de la matricula de la Facultad: "; cin>>CDMF;
   	
          	cout<<"\n Ingrese un numero para seleccionar otra Facultad, 0 para terminar: "; cin>>Num;
          	
          	
          	
       	if (RESE =='a')
		   {
          CM = matricula_1 (EFSS, EFCS, EFES, EFMS, EFCTS, EFIS);
          CMUS = calculate1 (EFSS, EFCS, EFES, EFMS, EFCTS, EFIS, CMSS, CMCS, CMES, CMMS, CMCTS, CMIS, CMUS);
         }
        else if (RESE =='b')
		{
          CM = matricula_2 (EFSO, EFCO, EFEO, EFMO, EFCTO, EFIO);
          CMUO = calculate2 (EFSO, EFCO, EFEO, EFMO, EFCTO, EFIO, CMUO, CMSO, CMCO, CMEO, CMMO, CMCTO, CMIO);
         } 
        else if (RESE =='c')
		{
         CM = matricula_3 (EFSC, EFCC, EFEC, EFMC, EFCTC, EFIC);
          CMUC = calculate3 (EFSC, EFCC, EFEC, EFMC, EFCTC, EFIC, CMSC, CMCC, CMEC, CMMC, CMCTC, CMIC, CMUC);
         } 
        else if (RESE =='d')
		{
          
         CM= matricula_4 (EFSCl, EFCCl, EFECl, EFMCl, EFCTCl, EFICl);
         CMUCl = calculate4 (EFSCl, EFCCl, EFECl, EFMCl, EFCTCl, EFICl, CMSCl, CMCCl, CMECl, CMMCl, CMCTCl, CMICl, CMUCl);
           }
        else if (RESE =='e')
		{
          CM = matricula_5 (EFSA, EFCA, EFEA, EFMA, EFCTA, EFIA);
          CMUA = calculate5 (EFSA, EFCA, EFEA, EFMA, EFCTA, EFIA, CMSA, CMCA, CMEA, CMMA, CMCTA, CMIA, CMUA);
          } 
        else if (RESE =='f')
		{
          CM = matricula_6 (EFSV, EFCV, EFEV, EFMV, EFCTV, EFIV);
          CMUV = calculate6 (EFSV, EFCV, EFEV, EFMV, EFCTV, EFIV, CMSV, CMCV, CMEV, CMMV, CMCTV, CMIV, CMUV);
          }
        else if (RESE =='g')
		{
          CM = matricula_7 (EFSCh, EFCCh, EFECh, EFMCh, EFCTCh, EFICh);
          CMUCh = calculate7 (EFSCh, EFCCh, EFECh, EFMCh, EFCTCh, EFICh, CMSCh, CMCCh, CMECh, CMMCh, CMCTCh, CMICh, CMUCh);
          }
        else if (RESE=='h')
		{
         CM = matricula_8 (EFSB, EFCB, EFEB, EFMB, EFCTB, EFIB);
         CMUB = calculate8 (EFSB, EFCB, EFEB, EFMB, EFCTB, EFIB, CMSB, CMCB, CMEB, CMMB, CMCTB, CMIB, CMUB);
          }
          monto (CM, CMUS, CMUO, CMUC, CMUCl, CMUA, CMUV, CMUCh, CMUB);
    	    
  } while (Num!=0);
  
  if (Num==0) {
     cout<<"\n Ingrese un numero para seleccionar otra sede/regional, 0 para finalizar: "; cin>>Num2;
 }
  } while (Num2!=0);
  
         students1 (EFSS, EFCS, EFES, EFMS, EFCTS, EFIS);
         matricula1 (CMSS, CMCS, CMES, CMMS, CMCTS, CMIS);
         students2 (EFSO, EFCO, EFEO, EFMO, EFCTO, EFIO);
         matricula2 (CMSO, CMCO, CMEO, CMMO, CMCTO, CMIO);
         students3 (EFSC, EFCC, EFEC, EFMC, EFCTC, EFIC);
         matricula3 (CMSC, CMCC, CMEC, CMMC, CMCTC, CMIC);
         students4 (EFSCl, EFCCl, EFECl, EFMCl, EFCTCl, EFICl);
         matricula4 (CMSCl, CMCCl, CMECl, CMMCl, CMCTCl, CMICl);
         students5 (EFSA, EFCA, EFEA, EFMA, EFCTA, EFIA);
         matricula5 (CMSA, CMCA, CMEA, CMMA, CMCTA, CMIA);
         students6 (EFSV, EFCV, EFEV, EFMV, EFCTV, EFIV);
         matricula6 (CMSV, CMCV, CMEV, CMMV, CMCTV, CMIV);
         students7 (EFSCh, EFCCh, EFECh, EFMCh, EFCTCh, EFICh);
         matricula7 (CMSCh, CMCCh, CMECh, CMMCh, CMCTCh, CMICh);
         students8 (EFSB, EFCB, EFEB, EFMB, EFCTB, EFIB);
         matricula8 (CMSB, CMCB, CMEB, CMMB, CMCTB, CMIB);
         students (CES, CES, CEC, CECl, CEA, CEV, CECh, CEB);
         matricula (CMUS, CMUO, CMUC, CMUCl, CMUA, CMUV, CMUCh, CMUB);
         prom (PMS, PMO, PMC, PMCl, PMA, PMV, PMCh, PMB);
  	     resultf (TCMTU, TMMCR, TMMeCR, CRMM, CRMeM);
  
    return 0;
}
